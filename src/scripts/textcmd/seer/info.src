  Info["CPU Quantity"]             := spy.cpu_quantity;
  Info["Memory"]                   := spy.memory+" MB";
  Info["Screen Resolution"]        := spy.screen_width+" x "+spy.screen_height+" x "+spy.screen_depth+" Bit";
