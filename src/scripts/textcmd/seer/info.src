function ShowNearMobs(who, Range)

  StandMultiSkLayout(20,10);
  GAddPageData(365,55,2650,2651,365,463,2648,2647);
  var Mobs := {};

  var i := 1;

  if(!Range)
    Range := 3;
  elseif(Range > 50 and !CmdHaveAccess(who, whom, CMDA_ADMIN))
