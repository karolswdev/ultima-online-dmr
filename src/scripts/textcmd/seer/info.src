    MOD_DEFENSE:
    MOD_TREE_SUPPORT:
    MOD_BLOCK_CAST:
    MOD_BLOCK_FIGHT:
    MOD_BLOCK_SELFHEAL:
    MOD_MONSTER_AR:
    MOD_SILVER_AR:
    MOD_FIRE_BLOOD:
    MOD_ONHIT_AR:
    MOD_LOWER_RC:
    MOD_FASTER_CAST:
    MOD_MANA_COST:
    MOD_SPELL_DMG:
    MOD_POISON_DMG:
    MOD_FIRE_DMG:
    MOD_COLD_DMG:
    MOD_LIGHT_DMG:
    MOD_RETORT:
    MOD_CRIT_HIT:
    MOD_STAM_DMG:
    MOD_DISARM:
    MOD_STUPOR:
