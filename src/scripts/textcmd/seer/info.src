  Info["Unique Instance ID of UO"] := Lower(Hex(spy.instance));
  Info["OS Version"]               := spy.os_major+"."+spy.os_minor+"."+spy.os_revision;
  Info["CPU Manufacturer"]         := spy.cpu_manufacturer;
  Info["CPU Family"]               := spy.cpu_family;
