function ProcessEraseMurdPenalty()

  var MurdData := GetMurdData(whom);
  MurdData.LastKill := 0;
  MurdData.Counter := 0;
  SetObjProperty(whom, PROP_MURDPNDATA, MurdData);
  CheckMurdPenalty(whom, 0);

endfunction

