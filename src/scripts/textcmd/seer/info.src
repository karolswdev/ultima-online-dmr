    MOD_TPREFIX:
    MOD_TSUFFIX:
    MOD_TGUILD:
    MOD_TRACE:
    MOD_INCO:         BuildTextModInfo(who, whom, ModId, hPos);

    MOD_MOUNT:        BuildNormalModInfo(who, whom, ModId, hPos, GetTempModAmount(whom,MOD_MOUNT)[1]+" "+GetTempModAmount(whom,MOD_MOUNT)[2]);

