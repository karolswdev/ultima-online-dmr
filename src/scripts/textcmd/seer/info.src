function CrMurdPenalty()

  var MurdData := struct;
  MurdData.+Counter    := 0;
  MurdData.+CurseMod   := 0;
  MurdData.+CurCurse   := 0;
  MurdData.+LastKill   := 0;
  MurdData.+LastVicSer := 0;
  MurdData.+Flags      := 0;
  SetObjProperty(whom, PROP_MURDPNDATA, MurdData);

endfunction

